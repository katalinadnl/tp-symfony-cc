{% extends 'base.html.twig' %}

{% block title %}Deliverable{% endblock %}

{% block body %}
    <h1>Deliverable</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ deliverable.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ deliverable.name }}</td>
            </tr>
            <tr>
                <th>Due_time</th>
                <td>{{ deliverable.dueTime ? deliverable.dueTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_deliverable_index') }}">back to list</a>

    <a href="{{ path('app_deliverable_edit', {'id': deliverable.id}) }}">edit</a>

    {{ include('deliverable/_delete_form.html.twig') }}
{% endblock %}
